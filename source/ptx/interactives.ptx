  \newcommand{\kgqm}[1]{#1~\mathrm{kg}/\mathrm{m}^3 }
  \newcommand{\Pa}[1]{#1~\mathrm{Pa} }
  \newcommand{\kPa}[1]{#1~\mathrm{kPa} }
  <!-- acceleration -->
  \newcommand{\aSI}[1]{#1~\mathrm{m}/\mathrm{s}^2 }
