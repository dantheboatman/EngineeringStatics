      <section xml:id="Interactive2-14">
        <title>3d-addition.ggb</title>
        <figure xml:id="figure_Interactive2-14">
          <caption>Interactive 2-14 Source: 3d-addition.ggb</caption>
          <interactive
           xml:id="ggb_3d-addition_interactive"
           aspect="10:6.5"
           platform="geogebra"
           preview="images/3d-addition.png"
           source="code/ggb_2-14.js"
           width="100%">
            <slate
             xml:id="ggb_2__14"
             aspect="10:5"
             source="images/3d-addition.ggb"
             surface="geogebra">
              <!-- B2,C2,D2 are x,y,z for Red point A, B3,C3,D3 are x,y,z for Blue point B  -->
              evalCommand("A=(3,10,4)");
